{% extends 'base.html.twig' %}

{% block title %}
    {{ song.artist }}
    -
    {{ song.title }}
{% endblock %}

{% block styleBody %}class="back-song" style="background-image: url('{{ song.photoAlbum }}')"
{% endblock %}

{% block body %}
